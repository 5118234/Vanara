    <AssemblyTitle>$(AssemblyName)</AssemblyTitle>
    <VersionPrefix>2.0.0</VersionPrefix>
    <TargetFrameworks>net20;net35;net40;net45;netstandard2.0</TargetFrameworks>
    <AssemblyName>Vanara.PInvoke.Security</AssemblyName>
